= form_for @volunteer do |f|
  -if @volunteer.errors.any?
    #error_explanation
      %h2= "#{pluralize(@volunteer.errors.count, "error")} prohibited this volunteer from being saved:"
      %ul
        - @volunteer.errors.full_messages.each do |msg|
          %li= msg

  .field
    = f.label :name
    = f.text_field :name
  .field
    = f.label :lat
    = f.text_field :lat
  .field
    = f.label :lng
    = f.text_field :lng
  .field
    = f.label :address
    = f.text_field :address
  .field
    = f.label :voice_number
    = f.text_field :voice_number
  .field
    = f.label :sms_number
    = f.text_field :sms_number
  .actions
    = f.submit 'Save'
