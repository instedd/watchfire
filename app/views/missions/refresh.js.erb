<%= render :partial => 'refresh_candidates' %>

$('#progress').html('<%= escape_javascript(render :partial => 'progress', :locals => {:mission => @mission} )%>')

$('#distance_canvas').html('<%=escape_javascript(render :partial => "distance", :locals => { :distance => @distance, :mission => @mission } )%>');

init_pause_checkbox();

<% if @distance %>
	setMapCircle(<%= @distance %>, true);
<% end %>

<% if @mission.finished? %>
	stop_refresh_enable_inputs();
<% end %>