$('#volunteers_canvas').html('<%=escape_javascript(render :partial => "candidates", :locals => { :mission => @mission } )%>');

$('#distance_canvas').html('<%=escape_javascript(render :partial => "distance", :locals => { :distance => @distance, :mission => @mission } )%>');

checkFilter($('input:radio[name=filter_btn]:checked').val());

init_pause_checkbox();

<% if @distance %>
	setMapCircle(<%= @distance %>, true);
<% end %>

<% if @mission.status_finished? %>
	stop_refresh_enable_inputs();
<% end %>
